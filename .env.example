# Optional: Year to download invoices for (defaults to current year)
AMZSC_INVOICE_YEAR=2025

# Optional: Download directory (defaults to ./downloads)
AMZSC_DOWNLOAD_DIR=./downloads

# Optional: User data directory for persistent browser session
# This stores your login session, cookies, etc.
AMZSC_USER_DATA_DIR=./browser-data

# Optional: Headless mode (false to see browser, true for background execution)
# Keep this false for first run to login manually
AMZSC_HEADLESS=false